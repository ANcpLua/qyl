<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net10.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <UserSecretsId>1113a6c1-021b-4956-9c51-239f56c18df4</UserSecretsId>
        <!-- Suppress analyzer warnings for demo code -->
        <NoWarn>$(NoWarn);CA1515;CA2000;CA2007;MA0004;MA0048</NoWarn>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Agents.AI" Version="1.0.0-preview.251204.1" />
        <PackageReference Include="Microsoft.Agents.AI.Abstractions" Version="1.0.0-preview.251204.1" />
        <PackageReference Include="Microsoft.Agents.AI.DevUI" Version="1.0.0-preview.251204.1"/>
        <PackageReference Include="Microsoft.Agents.AI.Hosting" Version="1.0.0-preview.251204.1" />
        <PackageReference Include="Microsoft.Agents.AI.Hosting.OpenAI" Version="1.0.0-alpha.251204.1"/>
        <PackageReference Include="Microsoft.Agents.AI.Workflows" Version="1.0.0-preview.251204.1" />
        <PackageReference Include="OllamaSharp" Version="5.4.12" />
        <!-- OpenTelemetry v1.38 GenAI Observability -->
        <PackageReference Include="OpenTelemetry" Version="1.14.0" />
        <PackageReference Include="OpenTelemetry.Extensions.Hosting" Version="1.14.0" />
        <PackageReference Include="OpenTelemetry.Exporter.Console" Version="1.14.0" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\qyl.sdk.aspnetcore\qyl.sdk.aspnetcore.csproj" />
        <ProjectReference Include="..\qyl.agents.telemetry\qyl.agents.telemetry.csproj" />
    </ItemGroup>

</Project>
