<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net10.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <!-- Suppress analyzer warnings for prototype code -->
        <NoWarn>$(NoWarn);CA1031;CA1034;CA1515;CA1716;CA1724;CA1849;CA2000;CA2007;CA2234;IL2066;MA0004;MA0048;MA0051</NoWarn>
    </PropertyGroup>

    <ItemGroup>
        <FrameworkReference Include="Microsoft.AspNetCore.App" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Agents.AI" Version="1.0.0-preview.251204.1" />
        <PackageReference Include="OpenTelemetry.Api" Version="1.14.0" />
        <PackageReference Include="Microsoft.Agents.AI.Abstractions" Version="1.0.0-preview.251204.1" />
        <PackageReference Include="Microsoft.Agents.AI.Hosting" Version="1.0.0-preview.251204.1" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\qyl.agents.telemetry\qyl.agents.telemetry.csproj" />
    </ItemGroup>

</Project>
